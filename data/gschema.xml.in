<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="@APP_PATH@" id="@APP_ID@">
    <key name="first-run" type="b">
      <default>true</default>
      <summary>First Run</summary>
      <description>Whether this is the first run of the application</description>
    </key>
    <key name="notifications-enabled" type="b">
      <default>false</default>
      <summary>Logging Notifications</summary>
      <description>Whether to notify the user to log their entries</description>
    </key>
    <key name="during-notification" type="b">
      <default>false</default>
      <summary>During Your Day Notification</summary>
      <description>Whether to notify the user to log their entries at a specified time, defaults to midday</description>
    </key>
    <key name="during-time" type="s">
      <default>'12:00'</default>
      <summary>Custom Time</summary>
      <description>The customizable time to notify user to log through the day</description>
    </key>
    <key name="end-notification" type="b">
      <default>false</default>
      <summary>End Of Day Notification</summary>
      <description>Whether to notify the user to log their entries at the end of the day</description>
    </key>
  </schema>
</schemalist>
